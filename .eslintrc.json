{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "eslint:recommended",
    "prettier"
  ],
  "overrides": [
    {
      "files": ["**/*.js?(x)"],
      "rules": {
        "react/destructuring-assignment": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "quotes": ["error", "single"],
        "import/no-extraneous-dependencies": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {}
}
